  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm-run-all -p dev-webpack dev-next dev-others",
    "dev-webpack": "webpack --config webpack.config.ts --mode development",
    "dev-next": "next dev",
    "dev-others": "concurrently \"npm:dev-data\" \"npm:dev-server\"",
    "dev-data": "ts-node --esm data.ts",
    "dev-server": "ts-node --esm server.ts",
    "build": "npm run clean && npm run build-ts && npm run build-swc && next build",
    "build-ts": "tsc",
    "build-swc": "swc ./src -d ./dist --source-maps",
    "clean": "rm -rf dist",
    "preview": "vite preview",
    "test": "mocha -r ts-node/register tests/**/*.ts",
    "start": "npm run build && echo '⚡️ Aether WEB3 CLI is starting the system!' && next start",
    "lint": "npx oxlint && next lint && npx eslint"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
